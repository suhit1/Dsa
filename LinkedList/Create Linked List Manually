public class Test {
    public static void main(String[] args) {

        Node head = new Node(0,null);
        // for storing the head of node
        Node temp = head;
        for (int i=1;i<=5;i++){
            Node newNode = new Node(i,null);
            head.next = newNode;
            head = head.next;
        }

        // iterating over linkedlist
        head = temp;
        head = head.next;
        while(head != null){
            System.out.print(head.n+" ");
            head = head.next;
        }
    }
}

class Node{
    int n;
    Node next;

    Node(int n,Node node){
        this.n = n;
        this.next = node;
    }
}
